<div class="posts list-group">
  {{#each posts}}
    <article class="card">
      <div class="card-header flex-row justify-space-between">
        <a
          class="list-item flex-row justify-space-between"
          href="/dashboard/update/{{id}}"
        >
          <span>{{title}}</span>
          <span>{{format_date createdAt}}</span>
        </a>
      </div>
    </article>
  {{/each}}
</div>